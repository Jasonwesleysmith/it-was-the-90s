<article class="card">
<h3 class="card-title">{{title}}</h3>
  <div class="card-image">
    {{!-- <img src="../images/d433c815b11b5b1c67adaa6c92a79abb.jpg"> --}}
    <div class="card-content">
      <a href="{{movie_url}}" target="_blank">{{movie_url}}</a>
      <p>{{vote_count}} people like this movie.</p>
      <p>{{reviews.length}} reviews</p>
      <a href="/movie/{{id}}">
        {{reviews.length}}
        review(s)
      </a>
    {{!-- </div> --}}
  </div>
</article>
