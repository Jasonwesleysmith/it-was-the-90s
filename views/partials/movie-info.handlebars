<h3 class="card-title">{{title}}</h3>
<div class="card-content">
  <a id="movieurl" href="{{movie_url}}" target="_blank">{{format_url
      movie_url
    }}</a>
  <p>{{vote_count}} people like this movie.</p>
  <a id="reviews-link" href="/movie/{{id}}">
    {{reviews.length}}
    {{format_plural "review" reviews.length}}
  </a>
</div>