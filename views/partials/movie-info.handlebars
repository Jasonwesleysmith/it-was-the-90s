<div class="row">
  <div class="col s12 m6">
    <div class="card">
      <div class="card-content black-text">
        <span class="card-title">Movie!</span>
        <article class="movie-post">
          <div class="title">
            <h3>{{title}}</h3>
            <span>{{movie_url}}</span>
          </div>
          <div>
            <p>{{vote_count}} people like this movie.</p>
            <p>{{reviews.length}} reviews</p>
          </div>
        </article>
      </div>
      <div class="card-action">
        <a href="#">This is a link</a>
        <p>{{reviews.length}} reviews</p>
      </div>
    </div>
  </div>
</div>